# TinaCMS Configuration
# Copy this file to .env.local and fill in your values

# TinaCloud Client ID (from app.tina.io project settings)
NEXT_PUBLIC_TINA_CLIENT_ID=

# TinaCloud Token (from app.tina.io project settings - Content API token)
TINA_TOKEN=

# App URL (for preview functionality)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# GitHub branch (auto-detected in most CI environments)
# GITHUB_BRANCH=main
