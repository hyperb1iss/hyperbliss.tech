{
  "name": "hyperbliss.tech",
  "version": "2.0.0",
  "description": "Stefanie's Personal Website",
  "author": "Stefanie Jane",
  "private": true,
  "scripts": {
    "prepare": "panda codegen",
    "dev": "panda codegen && panda cssgen --outfile styled-system/styles.css && tinacms dev -c \"next dev\"",
    "build": "tinacms build --skip-search-index && panda codegen && panda cssgen --outfile styled-system/styles.css && next build",
    "build:local": "tinacms build --local --skip-cloud-checks --skip-search-index && (tinacms dev --port 4001 --noWatch & sleep 1 && next build; kill $(lsof -t -i:4001) 2>/dev/null)",
    "start": "next start",
    "lint": "biome check . && prettier --check '**/*.{md,yml,yaml}'",
    "lint:all": "biome check --write --unsafe . && prettier --write '**/*.{md,yml,yaml}'",
    "lint:fix": "biome check --write . && prettier --write '**/*.{md,yml,yaml}'",
    "format": "biome format --write && prettier --write '**/*.{md,yml,yaml}'",
    "format:check": "biome format && prettier --check '**/*.{md,yml,yaml}'",
    "format:md": "prettier --write '**/*.md'",
    "format:yaml": "prettier --write '**/*.{yml,yaml}'",
    "check": "biome check && prettier --check '**/*.{md,yml,yaml}'",
    "ci": "biome ci && prettier --check '**/*.{md,yml,yaml}'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:seo": "vitest run tests/seo",
    "test:seo:watch": "vitest tests/seo",
    "analyze": "node scripts/analyze-bundle.mjs",
    "analyze:keep": "node scripts/analyze-bundle.mjs --keep"
  },
  "dependencies": {
    "feed": "5.1.0",
    "framer-motion": "^12.23.26",
    "gl-matrix": "3.4.4",
    "gray-matter": "^4.0.3",
    "hast-util-to-string": "^3.0.1",
    "next": "^16.1.1",
    "nextjs-google-analytics": "^2.3.7",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "5.5.0",
    "react-markdown": "10.1.0",
    "rehype-highlight": "7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "4.0.1",
    "tinacms": "^3.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@next/bundle-analyzer": "^16.1.1",
    "@pandacss/dev": "^1.8.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@tinacms/cli": "^2.0.6",
    "@types/hast": "^3.0.4",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "jsdom": "27.4.0",
    "playwright": "^1.57.0",
    "prettier": "3.7.4",
    "schema-dts": "^1.1.5",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "better-sqlite3",
      "esbuild",
      "sharp"
    ],
    "overrides": {
      "dompurify@<2.5.4": ">=2.5.4",
      "mermaid@<=10.9.2": ">=10.9.3",
      "esbuild@<=0.24.2": ">=0.25.0",
      "jsonpath-plus@<10.2.0": ">=10.2.0",
      "jsonpath-plus@<10.3.0": ">=10.3.0",
      "dompurify@<3.2.4": ">=3.2.4",
      "vite@<=5.4.19": ">=5.4.20",
      "vite@>=4.5.3 <5.0.0": ">=5.4.21",
      "dompurify@<2.4.2": ">=2.4.2",
      "dompurify@<2.5.0": ">=2.5.0"
    }
  }
}
